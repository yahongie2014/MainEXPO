{
  "expo": {
    "name": "framework",
    "slug": "framework",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "packagerOpts": {
      "assetExts": ["ttf", "otf", "wav", "mp4"]
    },

    "assetBundlePatterns": [
      "assets/*",
      "assets/images/*",
      "assets/fonts/*",
      "assets/sounds/*",
      "assets/images/icons/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.coder79",
      "infoPlist": {
        "NSLocationWhenInUseUsageDescription": "Your current location will be displayed on the map and used for directions"
      },
      "buildNumber": "1"
    },
    "android": {
      "config": {
        "googleMaps": {
          "apiKey": "AIzaSyAGF8cAOPFPIKCZYqxuibF9xx5XD4JBb84"
        }
      },
      "useNextNotificationsApi": true,
      "permissions": [
        "READ_EXTERNAL_STORAGE",
        "WRITE_EXTERNAL_STORAGE",
        "NOTIFICATIONS"
      ],
      "googleServicesFile": "./release/google-services.json",
      "package": "com.coder79",
      "versionCode": 2,
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#FFFFFF"
      }
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "platforms": ["android", "ios", "web"],
    "plugins": [
      [
        "expo-notifications",
        {
          "icon": "./assets/images/icons/notification-icon.png",
          "color": "#ffffff",
          "sounds": [
            "./assets/sounds/notification-sound.wav",
            "./assets/sounds/notification-sound.wav"
          ]
        }
      ]
    ],
    "description": "BaseProject"
  }
}
